<% if !logged_in? %>
<%= link_to 'Sign up', new_user_path %> | <%= link_to 'Sign in', signin_path %> |
<% else %>
<%= link_to 'See attractions', attractions_path %> | <%= link_to 'My Account', user_path(current_user) %> | 
<%= link_to 'Log Out', signout_path, method: :delete %> |
<% end %>
<% if current_user && is_admin? %>
<br />ADMIN: <%= link_to 'New Attraction', new_attraction_path  %>
<% end %>
